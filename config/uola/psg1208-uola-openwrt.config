CONFIG_TARGET_ROOTFS_PARTSIZE=1000
# IPV4地址设置
CONFIG_TARGET_PREINIT_IP="10.8.1.1"
CONFIG_TARGET_PREINIT_NETMASK="255.255.255.0"
CONFIG_TARGET_PREINIT_BROADCAST="10.8.1.255"
